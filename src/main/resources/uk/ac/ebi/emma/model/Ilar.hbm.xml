<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="uk.ac.ebi.emma.entity">
    <class name="Ilar" table="ilar">
        <id column="id" name="ilar_key">
            <generator class="native"/>
        </id>
        
        <property column="investigator" name="investigator"/>
        <property column="labcode"      name="labcode"/>
        <property column="organisation" name="organisation"/>
        <property column="status"       name="status"/>
    </class>
</hibernate-mapping>
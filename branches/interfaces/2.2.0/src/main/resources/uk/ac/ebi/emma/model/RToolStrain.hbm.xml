<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="uk.ac.ebi.emma.entity">
    <class name="RToolStrain" table="rtools_strains">
        <composite-id>        
            <key-many-to-one class="CvRTool" name="cvrtool" column="rtls_id" lazy="false" />
            <key-many-to-one class="Strain" name="strain" column="str_id_str" lazy="false" />
        </composite-id>
        
        <many-to-one cascade="all" class="Strain" name="strain" column="str_id_str" foreign-key="id_str" insert="false" lazy="false" not-found="ignore" update="false"/>
        <many-to-one cascade="all" class="CvRTool" name="cvrtool" column="rtls_id" insert="false" lazy="false" not-found="ignore" update="false"/>
        
        <property column="rtls_id" name="rtls_id" update="false" insert="false"/>
    </class>
</hibernate-mapping>
